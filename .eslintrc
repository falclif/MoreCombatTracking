{
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "extends": ["eslint:recommended"],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "no-case-declarations": 0,
        "no-extra-boolean-cast": 0,
        "no-useless-escape": 0
    },
    "plugins": []
}